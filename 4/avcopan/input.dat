molecule mol {
  0 1
  O
  H 1 0.96
  H 1 0.96 2 104.5
}

set basis         sto-3g
set e_convergence 10
set scf_type      pk
set maxiter       50
set mp2_type conv

basisname = psi4.get_global_option("BASIS")
basisset  = psi4.BasisSet.pyconstruct_orbital(mol, "BASIS", basisname)
mints     = psi4.MintsHelper(basisset)

# call RHF code
import sys
sys.path.insert(0, '../../3/avcopan/')
from rhf import RHF
rhf = RHF(mol, mints)
rhf.compute_energy()

# call RMP2 code
from rmp2 import RMP2
rmp2 = RMP2(rhf)

E = rmp2.compute_energy()

