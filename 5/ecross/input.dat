molecule mol{
1 2 
O 0.0 0.0 0.0
H 0.0 1.0 0.0
H 0.0 0.0 1.0
}

set { 
reference uhf
basis sto-3g
maxiter 300
scf_type pk
e_convergence 10
}

energy('scf')

basisname = psi4.get_global_option('BASIS')
basisset  = psi4.BasisSet.pyconstruct_orbital(mol, 'BASIS', basisname)
mints     = psi4.MintsHelper(basisset)

from uhf import UHF 
uhf = UHF(mol, mints)
